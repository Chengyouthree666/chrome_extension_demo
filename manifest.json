{
  "manifest_version": 3,
  "name": "chengyouthree",
  "version": "1.0.0",
  "description": "developer @chengyouthree",
  "background": {
    "service_worker": "bg.js"
  },
  "action": {
    "default_icon": {
      "19": "main.png"
    },
    "default_popup": "main.html",
    "default_title": "chengyouthree【chrome extension】"
  },
  "icons": {
    "16": "main.png",
    "48": "main.png",
    "128": "main.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ],
  "permissions": ["storage"]
}
